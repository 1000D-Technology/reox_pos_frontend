{
  "name": "reox_pos_frontend",
  "private": true,
  "version": "1.0.0",
  "description": "REOX POS System",
  "author": "1000D Technology",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none npm run dev\" \"wait-on http://localhost:5173 && cross-env NODE_ENV=development electron .\"",
    "electron:build": "cross-env ELECTRON_BUILDER_CACHE=./.builder-cache vite build && electron-builder",
    "dist": "npm run electron:build"
  },
  "build": {
    "appId": "com.reox.pos",
    "productName": "REOX POS",
    "files": [
      "dist/**/*",
      "electron/**/*",
      "package.json"
    ],
    "directories": {
      "output": "release"
    },
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "@heroui/breadcrumbs": "^2.2.21",
    "@heroui/system": "^2.4.22",
    "@heroui/theme": "^2.4.22",
    "@material-tailwind/react": "^2.1.10",
    "@tailwindcss/vite": "^4.1.14",
    "axios": "^1.13.2",
    "chart.js": "^4.5.0",
    "framer-motion": "^12.23.22",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.7",
    "lucide-react": "^0.544.0",
    "motion": "^12.23.22",
    "node-cron": "^4.2.1",
    "react": "^19.2.0",
    "react-barcode": "^1.6.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.2.0",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^7.9.3",
    "react-to-print": "^3.2.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "baseline-browser-mapping": "^2.9.14",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "date-fns": "^4.1.0",
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "wait-on": "^9.0.3"
  }
}
