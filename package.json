{
  "name": "reox_pos_frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron:dev": "concurrently --kill-others \"cd backend && node index.js\" \"vite\" \"wait-on http://127.0.0.1:5173 && wait-on http://127.0.0.1:3000/api/health && electron .\"",
    "electron:build": "npm run build && cd backend && npm install --production && cd .. && electron-builder",
    "electron:build:win": "npm run build && cd backend && npm install --production && cd .. && electron-builder --win",
    "pack": "npm run build && cd backend && npm install --production && cd .. && electron-builder --dir",
    "dist": "npm run build && cd backend && npm install --production && cd .. && electron-builder"
  },
  "dependencies": {
    "@heroui/breadcrumbs": "^2.2.21",
    "@heroui/system": "^2.4.22",
    "@heroui/theme": "^2.4.22",
    "@heroui/use-theme": "^2.1.10",
    "@material-tailwind/react": "^2.1.10",
    "@tailwindcss/vite": "^4.1.14",
    "axios": "^1.13.2",
    "chart.js": "^4.5.0",
    "classnames": "^2.5.1",
    "framer-motion": "^12.23.22",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.7",
    "lucide-react": "^0.544.0",
    "motion": "^12.23.22",
    "node-cron": "^4.2.1",
    "qrcode.react": "^4.2.0",
    "react": "^19.2.0",
    "react-barcode": "^1.6.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.2.0",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^7.9.3",
    "react-to-print": "^3.2.0",
    "xlsx": "^0.18.5",
    "electron-updater": "^6.1.7",
    "electron-log": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "baseline-browser-mapping": "^2.9.14",
